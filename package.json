{
  "name": "food-app-backend",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "module": "common",
  "scripts": {
    "start:prod": "node dist/bundle.js",
    "start:dev": "concurrently \"npm run build:dev && sleep 5\" \"npm run dev\" --raw",
    "dev:debug": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/index.ts --no-clear",
    "build": "webpack --mode production",
    "build:dev": "tsc --watch --noEmit",
    "dev": "npx ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --clear src/index.ts",
    "lint": "npx eslint src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "engines": {
    "node": "16.x"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@types/chalk": "^2.2.4",
    "apollo-server": "^3.13.0",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cli-color": "^2.0.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "graphql": "^16.9.0",
    "graphql-date-scalars": "^0.2.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^17.0.0",
    "graphql-ws": "^5.16.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "mongoose": "^8.7.1",
    "mongoose-paginate-v2": "^1.8.5",
    "multer": "^1.4.5-lts.1",
    "telegraf": "^4.16.3",
    "typescript": "^5.6.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express-fileupload": "^1.5.1",
    "@types/graphql-upload": "^16.0.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.12",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.0",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript-eslint": "^8.9.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
